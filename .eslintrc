{
  "extends": [
    "airbnb",
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "import-helpers"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ],
    "import-helpers/order-imports": [
      "warn",
      {
        "newlinesBetween": "always",
        "groups": [
          "/^react/",
          "module",
          [
            "parent",
            "sibling",
            "index"
          ]
        ],
        "alphabetize": {
          "order": "asc",
          "ignoreCase": true
        }
      }
    ],
    "react/jsx-props-no-spreading": "off"
  }
}
